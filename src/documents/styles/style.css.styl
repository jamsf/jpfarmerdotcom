@import 'nib'

a, a:link
	cursor pointer

body
	padding-top 60px
	color #EEEEEE
	background-image url('/images/bg.png');
	background-repeat no-repeat
	background-attachment fixed

.style20
	font-size 12px

.style23
	font-size 12
	align left

table
	border-collapse: separate;
	border-spacing: 4px;
	border-color: gray;
	tfoot, tbody
		th
			border-top 1px solid #DDD
